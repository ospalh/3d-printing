breite = 129.39;
tiefe = 67.08;


rand = 5;
hw = 2;  // Halter weite
hh = 0.5;

//schwaebische_gruesse(1); // lose
// positiver_Gruss();
negativer_Gruss();
// schneller_Gruss();

module positiver_Gruss()
{
   Platte(1.2);
   translate([0,0,1.2])
   {
      schwaebische_gruesse(0.4);
   }
}

module negativer_Gruss()
{
   difference()
   {
      Platte(2);
      translate([0, 0, 0.9])
      {
         schwaebische_gruesse(1.2+fudge);
      }
   }
}

module schneller_Gruss()
{
   Halter();
   schwaebische_gruesse(1+hh);
}



module schwaebische_gruesse(h)
{
   mirror([0,1,0])
   {
      schwaebische_gruesse_kopfueber(h);
   }
}

module schwaebische_gruesse_kopfueber(h)
{
   difference()
   {
      linear_extrude(height=h)
      {
         Lecken_mich_voll();
      }
      translate([0,  0,  -fudge])
      {
         linear_extrude(height=h+2*fudge)
         {
            L_punze();
            e_1_punze_2();
            e_1_punze_1();
            c_1_punze();
            k_punze_1();
            k_punze_2();
            k_punze_3();
            e_2_punze_1();
            e_2_punze_2();
            c_2_punze();
         }
      }
   }
   difference()
   {
      linear_extrude(height=h)
      {
         Sie_voll();
      }
      translate([0,  0,  -fudge])
      {
         linear_extrude(height=h+2*fudge)
         {
            S_punze_1();
            S_punze_2();
            e_3_punze();
         }
      }
   }
   difference()
   {
      linear_extrude(height=h)
      {
         am_voll();
      }
      translate([0,  0,  -fudge])
      {
         linear_extrude(height=h+2*fudge)
         {
            a_punze();
         }
      }
   }
   difference()
   {
      linear_extrude(height=h)
      {
         sch_voll();
      }
      translate([0,  0,  -fudge])
      {
         linear_extrude(height=h+2*fudge)
         {
            c_3_punze();
         }
      }
   }
   linear_extrude(height=h)
   {
      Ar();
   }
   linear_extrude(height=h)
   {
      bang_schaft();
   }
   linear_extrude(height=h)
   {
      bang_punkt();
   }

}

